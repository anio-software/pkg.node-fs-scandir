{
    "extends": "./tsconfig.base.json",
    "compilerOptions": {
        "paths": {
            "#~src/*": [
                "./src/*"
            ],
            "#~auto/fourtune/*": [
                "./auto/fourtune/src/*"
            ],
            "#~auto/user/*": [
                "./auto/user/src/*"
            ],
            "#~assets/*": [
                "./assets/*"
            ]
        }
    },
    "include": [
        "../../../auto/fourtune/src/**/*",
        "../../../auto/user/src/**/*"
    ]
}
